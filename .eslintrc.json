{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": 0,
        "linebreak-style": 0,
        "indent": [2, 4],
        "no-param-reassign": ["error", { "props": false }]
    }
}
